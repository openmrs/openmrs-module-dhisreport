<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.dhisreport.api.model">
    <class name="DataValueTemplate" table="${project.parent.artifactId}_datavalue_template">
        <id column="id" name="id" unsaved-value="0">
            <generator class="native"/>
        </id>
        <discriminator column="id" insert="false"/>
        <many-to-one name="dataElement" column="dataelement_id" not-null="true"/>
        <many-to-one name="dataSet" column="dataset_id" not-null="true"/>
        <property column="reportIndicatorUuid" name="reportIndicatorUuid"/>
        <set name="disaggregations" table="${project.parent.artifactId}_datavalue_template_disaggregation"
          lazy="true" fetch="select" cascade="all">
            <key>
                <column name="datavalue_template_id" not-null="true"/>
            </key>
            <many-to-many class="Disaggregation">
                <column name="disaggregation_id" not-null="true"/>
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>
