<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.dhisreport.api.model">
    <class name="DataValueTemplate" table="${project.parent.artifactId}_datavalue_template">
        <id column="id" name="id" unsaved-value="0">
            <generator class="native"/>
        </id>
        <discriminator column="id" insert="false"/>
        <many-to-one name="dataElement" column="data_element_id" not-null="true"/>
        <many-to-one name="dataSet" column="dataset_id" not-null="true"/>
        <property column="name" name="name"/>
        <property column="reportDefinitionUuid" name="reportDefinitionUuid"/>
        <property column="reportDefinitionLabel" name="reportDefinitionLabel"/>
        <set name="disaggregations" table="${project.parent.artifactId}_datavalue_template_disaggregation"
          lazy="true" fetch="select" cascade="all">
            <key>
                <column name="datavalue_template_id" not-null="true"/>
            </key>
            <one-to-many class="Disaggregation"/>
        </set>
    </class>
</hibernate-mapping>
