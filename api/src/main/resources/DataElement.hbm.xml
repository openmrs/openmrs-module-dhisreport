<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.dhisreport.api.model">
    <class name="DataElement" table="${project.parent.artifactId}_dataelement">
        <id column="id" name="id" unsaved-value="0">
            <generator class="native"/>
        </id>
        <discriminator column="id" insert="false"/>
        <property column="name" name="name"/>
        <property column="uid" name="uid"/>
        <property column="code" name="code"/>
        <set name="dataSets" table="${project.parent.artifactId}_dataset_dataelement"
          lazy="true" fetch="select" cascade="all">
            <key>
                <column name="dataelement_id" not-null="true"/>
            </key>
            <many-to-many class="DataSet">
                <column name="dataset_id" not-null="true"/>
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>
